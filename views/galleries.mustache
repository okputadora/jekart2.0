<!DOCTYPE html>
<html dir="ltr" lang="en-US">
{{>head }}
<body class="stretched">
	<div id="wrapper" class="clearfix" style>
		{{>nav }}
		<div id="page-menu" class="no-sticky">
			<div id="" class='custom-page-banner'>
				<div class="container clearfix">
					<div class="menu-title"><span>galleries</span></div>
				</div>
			</div>
		</div>
		<div class="custom-shop-banner">
			<h2>
				 Interested in purchasing original art? <a href="/contact"><strong>Send me a message!</strong></a>
			</h2>
		</div>
		<section id="content" style="width:100%;">
			<div class="content-wrap custom-background">
				<div class="container clearfix">
					<div class="clear"></div>
				<div id="galleries" class="flex-container">
					{{#galleries}}
					<a href='/gallery/{{name}}'><div id="gallery-{{name}}" class="gallery">
						<img src="/art_imgs/{{name}}/{{imagePath}}" class="gallery-image"/>
						<div class="gallery-description"><div class="gallery-title">{{name}}</div></div>
					</div></a>
					{{/galleries}}
				</div>
				</div>
			</div>
			<div id="gotoTop" class="icon-angle-up"></div>
		</section>
		{{>footer}}
	</div>
	<script src="/build/vendor.min.js"></script>
</body>
</html>
