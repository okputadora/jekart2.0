<!DOCTYPE html>
<html dir="ltr" lang="en-US">
{{> head }}

<body class="stretched">

	<!-- Document Wrapper
	============================================= -->

	<div id="wrapper" class="clearfix">

    {{> nav }}
    <div id="page-menu" class="no-sticky" style="background: #1b1b1b">
      <div id="" class='custom-page-banner'style="position: fixed; width: 100%;">
        <div class="container clearfix">
          <div style="padding-left: 10px;"class="menu-title" style='width: 100%'><a href='/shop'>shop </a><img style="margin:5px;" src="/images/icons/submenu.png"><span>cart</span></div>
          <a href=""><i class="icon-shopping-cart right custom-cart"></i></a>
        </div>
      </div>
    </div>

    <section style="min-height: 80vh" id="content">

			<div class="content-wrap">

				<div class="container clearfix">

					<div id="cart-table" class="table-responsive">
						<table class="table cart shop-container">
							<thead>
								<tr>
									<th class="cart-product-remove">&nbsp;</th>
									<th class="cart-product-thumbnail">&nbsp;</th>
									<th class="cart-product-name">Product</th>
									<th class="cart-product-price">Price</th>
									<th class="cart-product-quantity">Quantity</th>
									<th class="cart-product-subtotal">Total</th>
								</tr>
							</thead>
							<tbody id="cart-list">
							</tbody>

						</table>
						<a href="/shop/checkout" class="button shop-item-add-to-cart m-4 custom-shadow">Checkout</a>
						</div>
					</div>



				</div>

			</div>

		</section><!-- #content end -->


    {{> footer }}
	</div>
  </body>
  </html>
